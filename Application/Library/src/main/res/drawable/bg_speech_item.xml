<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="1px" android:color="#a4b1bc" />
            <solid android:color="@color/color_item_increment_press_bg" />
            <corners android:radius="2dp" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <solid android:color="@color/color_item_increment_normal_bg" />
            <corners android:radius="2dp" />
            <stroke android:width="1px" android:color="#a4b1bc" />
        </shape>
    </item>
</selector>